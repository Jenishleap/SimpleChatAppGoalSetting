<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leapfrog.simplechat_goalsetting">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".firebase.onetoone.login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".firebase.group.FireBaseActivity"></activity>
        <activity android:name=".firebase.onetoone.register.Register"></activity>
        <activity android:name=".firebase.onetoone.Users"></activity>
        <activity android:name=".firebase.onetoone.Chat"></activity>

    </application>


</manifest>